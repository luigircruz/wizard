{
    "name": "luigircruz/wizard",
    "description": "A simple landing page generator cli.",
    "keywords": ["wizard", "console", "cli"],
    "homepage": "https://luigicruz.dev",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/luigircruz/wizard/issues",
        "source": "https://github.com/luigircruz/wizard"
    },
    "authors": [
        {
            "name": "Luigi Cruz",
            "email": "luigicruz.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/framework": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["wizard"]
}
